Indexes:
    "analises_quimicas_pkey" PRIMARY KEY, btree (id)
    "analises_quimicas_numeroAnalise_key" UNIQUE, btree ("numeroAnalise")
Foreign-key constraints:
    "analises_quimicas_clienteId_fkey" FOREIGN KEY ("clienteId") REFERENCES pessoas(id) ON UPDATE CASCADE ON DELETE CASCADE
    "analises_quimicas_organizationId_fkey" FOREIGN KEY ("organizationId") REFERENCES "Organization"(id) ON UPDATE CASCADE ON DELETE CASCADE
