{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "preprocess:sales": "tsx src/json-imports/preprocess-sales.ts",
    "db:backfill-quotations": "dotenv -e ../../.env -- ts-node prisma/backfill-quotations.ts",
    "db:import-sales": "dotenv -e ../../.env -- ts-node prisma/import-sales.ts",
    "db:backfill-quotations-from-items": "dotenv -e ../../.env -- ts-node prisma/backfill-quotations-from-items.ts",
    "db:import-clients-from-csv": "dotenv -e ../../.env -- ts-node prisma/import-clients-from-csv.ts",
    "db:recalculate-gold-amount": "dotenv -e ../../.env -- ts-node prisma/recalculate-gold-amount.ts",
    "db:link-legacy-sales": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register prisma/link-legacy-sales.ts",
    "db:recalculate-adjustments": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register prisma/recalculate-sale-adjustments.ts",
    "db:set-adjustment-methods": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register prisma/set-adjustment-methods.ts",
    "db:diagnose-pg": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register prisma/diagnose-product-groups.ts",
    "copy:prisma-engine": "cp ../../node_modules/.prisma/client/libquery_engine-debian-openssl-3.0.x.so.node ./dist/",
    "db:export-seed-data": "dotenv -e ../../.env -- ts-node prisma/export-seed-data.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.11.0",
    "@sistema-erp-electrosal/core": "workspace:*",
    "@types/passport-local": "^1.0.38",
    "@types/pdfjs-dist": "^2.10.378",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.2",
    "handlebars": "^4.7.8",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "ofx-js": "^0.2.0",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfjs-dist": "^5.4.54",
    "puppeteer": "^22.15.0",
    "reflect-metadata": "^0.2.2",
